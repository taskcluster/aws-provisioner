{
  "name": "taskcluster-aws-provisioner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "compile": "rm -rf bin lib && ./node_modules/.bin/babel -s -d lib src/lib && ./node_modules/.bin/babel -s -d bin src/bin",
    "install": "npm run compile",
    "pretest": "npm run compile",
    "test": "test/runtests.sh"
  },
  "babel": {
    "sourceMaps": true,
    "optional": [
      "runtime"
    ],
    "stage": 1
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taskcluster/aws-provisioner.git"
  },
  "dependencies": {
    "bluebird": "^2.9.34",
    "commander": "^2.6.0",
    "debug": "^2.2.0",
    "kexec": "^1.2.0",
    "knuth-shuffle": "^1.0.1",
    "lodash": "^3.10.1",
    "multi-region-promised-aws": "0.0.5",
    "node-uuid": "^1.4.2",
    "promise": "^7.0.4",
    "sinon": "^1.15.4",
    "slugid": "^1.1.0",
    "taskcluster-base": "^0.8.6",
    "taskcluster-client": "^0.22.6"
  },
  "devDependencies": {
    "assume": "^1.2.5",
    "babel": "^5.8.23",
    "babel-eslint": "^4.0.5",
    "babel-runtime": "^5.8.25",
    "eslint": "^1.0.0",
    "mocha": "^2.2.5",
    "should": "^7.0.2"
  },
  "engines": {
    "node": "0.12.x"
  }
}
